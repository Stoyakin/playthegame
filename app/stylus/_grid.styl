.container
  width 100%
  margin 0 auto
  @media $desktop
    max-width 1270px
  @media $desktopMax
    max-width 1140px
  @media $desktopToTouch
    max-width 974px
  @media $tPortraitMax
    max-width 718px
  @media $mLandscapeMax
    max-width calc(100% - 50px)
  @media (max-width: 400px)
    max-width calc(100% - 30px)
